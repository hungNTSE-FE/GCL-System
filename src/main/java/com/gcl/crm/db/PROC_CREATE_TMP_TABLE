DROP PROCEDURE IF EXISTS PROC_CREATE_TMP_TABLE;
CREATE PROCEDURE PROC_CREATE_TMP_TABLE(IN TABLE_NAME varchar(50))
BEGIN
    CASE TABLE_NAME
        WHEN 'TMP_KPI_EMPLOYEE' THEN
            DROP  TEMPORARY TABLE IF EXISTS TMP_KPI_EMPLOYEE;
            CREATE TEMPORARY TABLE TMP_KPI_EMPLOYEE (
              EMPLOYEE_ID INT NOT NULL,
              EMPLOYEE_NAME VARCHAR(50) NULL,
              SUM_CUS_DATA INT DEFAULT 0,
              SUM_LOT INT DEFAULT 0,
              KPI DOUBLE DEFAULT 0,
              PRIMARY KEY (EMPLOYEE_ID)
            );
    END CASE;
END;

